{
  "name": "react-luxoft-starter",
  "version": "2.0.0",
  "description": "Starter kit for creating apps with React and Redux",
  "engines": {
    "npm": ">=4"
  },
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "setup": "node tools/setup/setupMessage.js && npm install && node tools/setup/setup.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message",
    "start": "npm-run-all --parallel test:watch lint:watch \"open:src -- {1} {2} {3}\" --",
    "open:src": "babel-node tools/srcServer.js",
    "open:mocks": "babel-node tools/mockServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist && npm run lint && npm run test",
    "build": "babel-node tools/build.js",
    "test": "jest --config jest.config.json",
    "test:CI": "babel-node tools/testCi.js",
    "test:cover": "npm run test -- --coverage ",
    "test:cover:CI": "npm run test:CI -- --coverage && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "npm run test -- --watch",
    "open:cover": "npm run test:cover && open coverage/lcov-report/index.html",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js"
  },
  "author": "Dmitriy Yurov & Dennis Baizulin",
  "license": "MIT",
  "dependencies": {
    "axios": "0.16.2",
    "immutable": "3.8.1",
    "json-schema-faker": "0.5.0-rc11",
    "node-gyp": "3.6.2",
    "object-assign": "4.1.1",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-redux": "5.0.6",
    "react-router": "4.2.0",
    "react-router-redux": "4.0.8",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "7.1.4",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.8",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-latest": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-react-optimize": "1.0.1",
    "babel-preset-stage-0": "6.24.1",
    "browser-sync": "2.18.13",
    "chalk": "2.1.0",
    "connect-history-api-fallback": "1.3.0",
    "coveralls": "2.13.2",
    "css-loader": "0.28.7",
    "enzyme": "3.0.0",
    "eslint": "4.7.2",
    "eslint-import-resolver-webpack": "0.8.3",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-react": "7.4.0",
    "eslint-watch": "3.1.2",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "html-webpack-plugin": "2.30.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "21.2.1",
    "json-loader": "0.5.7",
    "mockdate": "2.0.2",
    "node-sass": "4.5.3",
    "npm-run-all": "4.1.1",
    "open": "0.0.5",
    "postcss-loader": "2.0.6",
    "prompt": "1.0.0",
    "prop-types": "15.6.0",
    "proxy-middleware": "0.15.0",
    "react-addons-test-utils": "15.6.2",
    "redux-immutable-state-invariant": "2.0.0",
    "redux-logger": "3.0.6",
    "replace": "0.3.0",
    "resolve-url-loader": "2.1.0",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "style-loader": "0.18.2",
    "url-loader": "0.5.9",
    "url-pattern": "1.0.3",
    "webpack": "3.6.0",
    "webpack-bundle-analyzer": "2.9.0",
    "webpack-dev-middleware": "1.12.0",
    "webpack-hot-middleware": "2.19.1",
    "webpack-md5-hash": "0.0.5"
  },
  "keywords": [
    "react",
    "reactjs",
    "react-router",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "redux",
    "flux",
    "boilerplate",
    "starter"
  ]
}
